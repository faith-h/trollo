<h1><%= @board.name %></h1>
<div class="card-panel hoverable">
<% @board.lists.each do |list| %>
  
        <h1><%= link_to list.name, [@board, list] %></h1>
<% end %>
  </div>

  <%= link_to "Add List", new_board_list_path(@board) %>
<%= link_to 'Edit Board', edit_board_path(@board) %>
