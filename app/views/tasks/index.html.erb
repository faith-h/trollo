<h1>Tasks</h1>
<% @tasks.each do |task| %>
  <div class="card-panel hoverable">
        <h1><%= task.name %></h1>
        <h3><%= task.desc %></h3>
        <%= link_to "Add Task", new_list_task_path(@list) %>
        <%= link_to "Delete Task", [@list, task], method: :delete %>
        <%= link_to "Back", boards_path %>
<% end %>
  </div>



